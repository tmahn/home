#!/usr/bin/env python
# coding: UTF-8

from BeautifulSoup import BeautifulSoup
from mechanize import Browser

br = Browser()
br.open('http://msdn04.e-academy.com/elms/Storefront/Storefront.aspx?campus=ieee_cs_r9')

response = br.response().get_data()
soup = BeautifulSoup(response, convertEntities=BeautifulSoup.HTML_ENTITIES)

print '\n'.join(option.string 
                    for option in soup.findAll(id='ddlProductPickList')[0]\
                        .findAll('option'))
