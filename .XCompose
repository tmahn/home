<Multi_key> <less> <3>			: U2764 # HEAVY BLACK HEART

<Multi_key> <minus> <less> 		: U2190 # LEFTWARDS ARROW
<Multi_key> <minus> <asciicircum> 	: U2191 # UPWARDS ARROW
